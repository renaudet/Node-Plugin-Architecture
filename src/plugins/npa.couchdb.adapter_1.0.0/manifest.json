{
  "id": "npa.couchdb.adapter",
  "name": "CouchDB support for NPA",
  "version": "1.0.0",
  "plugin": "plugin.js",
  "requires": [
  	{"type": "plugin","id": "npa.core","version": "1.0.0"},
  	{"type": "plugin","id": "npa.logging","version": "1.0.0"},
  	{"type": "plugin","id": "npa.telemetry","version": "1.0.0"}
  ],
  "extends": [
  	{
  		"point": "npa.log.provider",
  		"id": "npa.couchdb.adapter.logger",
  		"dir": "npa.couchdb.adapter"
  	},
  	{
  		"point": "npa.core.service",
  		"id": "npa.couchdb.adapter.service",
  		"service": "couchdb"
  	},
  	{
  		"point": "npa.telemetry.dimension.provider",
  		"id": "npa.couchdb.adapter.stat.provider",
		"name": "database.transaction",
		"displayLabel": "Database Transactions",
		"description": "Number of database transaction per inteval (30sec.)",
		"type": "int",
		"unit": "",
		"maxPoints": 60
  	}
  ],
  "provides": [
  	{"id": "npa.couchdb.adapter.datasource"}
  ]
}